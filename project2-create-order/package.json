{
  "name": "project2-create-order",
  "version": "1.0.0",
  "main": "app.js",
  "license": "MIT",
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "dev": "DEBUG=project2-create-order:* NODE_ENV=development nodemon --inspect app.js",
    "migrate": "knex migrate:latest",
    "seeds": "NODE_ENV=development node db/seeds/project2-create-order.js",
    "migrate-test": "knex --env test migrate:latest",
    "seeds-test": "NODE_ENV=test node db/seeds/project2-create-order.js",
    "tester": "NODE_ENV=test mocha \"routes/*.test.js\"",
    "test": "yarn seeds-test && concurrently -k \"NODE_ENV=test PORT=3002 node app.js\" \"yarn tester\"",
    "lint": "yarn eslint .",
    "cleanup": "node ./cleanup.js"
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "bookshelf": "^0.10.4",
    "cfenv": "^1.0.4",
    "chalk": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "debug": "~2.2.0",
    "express": "^4.15.4",
    "fs-extra": "^4.0.1",
    "knex": "^0.13.0",
    "morgan": "~1.7.0",
    "pg": "^7.1.0"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  }
}
